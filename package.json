{
  "name": "novelcovid-scraper",
  "version": "1.0.0",
  "description": "A simple Node API for getting coronavirus cases from scraped information.",
  "main": "start.js",
  "author": "Kavin Varnan <skavinvarnan@gmail.com>",
  "repository": {
    "url": "https://github.com/COVID19-SARS-CoV-2/covid-19-scraper"
  },
  "license": "MIT",
  "keywords": [
    "API",
    "coronavirus",
    "covid"
  ],
  "eslintIgnore": [
    "**/*.spec.*"
  ],
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon --watch routes --watch scrapers --watch utils ./server.js ",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "lint:fix": "eslint ./**/*.js --fix --ignore-pattern node_modules/",
    "lint:win32": "eslint ./**/*.js --ignore-pattern node_modules/",
    "test": "mocha ./tests --exit  --timeout 50000"
  },
  "dependencies": {
    "@sentry/node": "5.12.2",
    "axios": "^0.19.2",
    "cheerio": "^0.22.0",
    "cookie-parser": "1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csurf": "1.11.0",
    "csvtojson": "^2.0.10",
    "ejs": "3.0.2",
    "express": "^4.16.3",
    "ioredis": "*",
    "mailgun-js": "^0.22.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@aero/eslint-config": "^1.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2"
  }
}
